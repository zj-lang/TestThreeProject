<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.qfedu.dao.CarDao">
    <select id="select" parameterType="int" resultType="com.qfedu.entity.City">
        select * from sys_city where pid = #{pid}
    </select>

    <select id="selectByCityId" parameterType="int" resultType="com.qfedu.entity.Car">
        select * from sys_car where cid = #{cid}
    </select>

    <select id="selectMsg" parameterType="int" resultType="com.qfedu.entity.City">
        select * from sys_city where id = #{id}
    </select>

</mapper>